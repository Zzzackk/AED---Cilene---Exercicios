#include <stdio.h>
#define TM 10

void main()
{
    float n[TM], nQuadrado[TM];

    for (int i=0; i<TM; i++) {
        printf("Digite um valor n[%d]: ", i);
        scanf("%f", &n[i]);
    }

    for (int i=0; i<TM;i++){
        nQuadrado[i] = (n[i] * n[i]);
        printf("valor de n[%d]: %.1f e o seu quadrado Ã©: %.1f\n", i, n[i], nQuadrado[i]);
    }
}

